@{
    ViewData["Title"] = "ADRES";

}

<div class="LoginContent">
    <div class="ImagenLogin">
        <img src="Img/logo-adres.svg"  />
    </div>
    <div class="Login">
        <div class="ContetLogin">
            <form id="loginForm">
                <div class="mb-3 imagenUser">
                    <img src="Img/user.png" height="100px" />
                </div>
                <div class="mb-3">
                    <label class="form-label">Usuario</label>
                    <input class="form-control" id="Usuario" placeholder="Usuario" required>
                </div>
                <div class="mb-3">
                    <label class="form-label">Contraseña</label>
                    <input type="password" class="form-control" id="Contraseña" placeholder="Contraseña" required>
                </div>
                <button type="submit" class="btn btn-primary">Ingresar</button>
            </form>
        </div>
    </div>
</div>
